{
    "version": 2,
    "name": "auth-nodejs-rest-api",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/routes/auth.routes.js"
        },
        {
            "src": "/(.*)",
            "dest": "/routes/user.routes.js"
        }
    ]
}